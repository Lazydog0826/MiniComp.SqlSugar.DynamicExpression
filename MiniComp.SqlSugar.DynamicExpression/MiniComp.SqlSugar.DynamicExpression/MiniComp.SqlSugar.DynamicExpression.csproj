<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>90.0</Version>
    <Authors>Lazydog0826</Authors>
    <PackageProjectUrl>https://github.com/Lazydog0826/MiniComp.SqlSugar.DynamicExpression</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Lazydog0826/MiniComp.SqlSugar.DynamicExpression</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath>.</PackagePath>
    </None>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>.</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="MiniComp.Core" Version="9.0.0" />
  </ItemGroup>
</Project>
